{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2020",
    "allowJs": true,
    "declaration": true,
    "jsx": "react"
  },
  "include": ["src"]
}
